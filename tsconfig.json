{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "module":"es2015",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "strict": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "checkJs": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declarationDir": "dist/types",
    "outDir": "dist/lib",
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "src", "generated"
  ]
}
